#TO MAKE A FUNCTION FOR THE SWEET BROWNIE POINTS
dataprocessor <- function(namesto, valuesto){
pivot_longer(
cols = everything(),
names_to = namesto,
values_to = valuesto) %>%
rename(region = Country.Code)
}
